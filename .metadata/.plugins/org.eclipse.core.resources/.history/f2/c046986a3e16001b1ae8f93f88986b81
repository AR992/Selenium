public class MyClass {
	
	public static void main(String[] args) {
		
		// System.setProperty("webdriver.chrome.driver","C:\\Folder for testing\\JARS-selenium\\chromedriver_win32\\chromedriver.exe");
		System.setProperty("webdriver.gecko.driver",":\\Folder for testing\\selenium2\\newProject\\lib\\geckoDriver\\geckodriver.exe");
		// WebDriver driver = new ChromeDriver();
		WebDriver driver = new FirefoxDriver();
		
		driver.get("https://www.guru99.com/first-webdriver-script.html");
		driver.quit();
	}

}
